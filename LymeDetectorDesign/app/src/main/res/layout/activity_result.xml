<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kamilsaitov.LymeDetector.ResultActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <io.netopen.hotbitmapgg.library.view.RingProgressBar
        android:id="@+id/result_bar"
        android:layout_width="307dp"
        android:layout_height="382dp"
        android:layout_marginEnd="2dp"
        android:rotation="270"
        app:max="100"
        app:ringColor="#696969"
        app:ringProgressColor="#cb8638"
        app:ringWidth="25dp"
        app:style="STROKE"
        app:textColor="@color/colorPrimary"
        app:textIsShow="false"
        app:textSize="16sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="6dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lyme confidence:"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="87dp"
        android:layout_marginTop="42dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="87dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="419dp"
        android:layout_height="0dp"
        app:srcCompat="@android:color/background_light"
        android:tint="#fafafa"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/result_bar"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="35dp"
        app:layout_constraintTop_toTopOf="@+id/resultView"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/resultView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="50dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="152dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/result_bar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Remember!"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="30dp" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="106dp"
        android:text="The app can only be so good. Although this app was created in cooperation with specialists, it can 'miss' sometimes. Please consider consulting a doctor and taking proper tests for better results."
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        android:layout_marginEnd="31dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="31dp"
        android:layout_marginRight="31dp"
        android:id="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteY="401dp"
        tools:layout_editor_absoluteX="33dp" />

</android.support.constraint.ConstraintLayout>
